<odoo>
  <template id="report_invoice_document_inherit"
            inherit_id="account.report_invoice_document"
            priority="20">
    <!-- Busca cualquier llamada al layout nativo -->
    <xpath expr="//t[starts-with(@t-call,'web.external_layout')]" position="replace">
      <!-- Llama a tu layout que hace el t-call a web.external_layout -->
      <t t-call="lims_accounting_extension.external_layout_invoice_lims"/>
    </xpath>
  </template>
</odoo>
