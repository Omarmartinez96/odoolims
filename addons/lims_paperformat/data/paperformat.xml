<odoo>
  <data noupdate="0">
    
    <record id="paperformat_lims_letter" model="report.paperformat">
      <field name="name">Carta (LIMS)</field>
      <field name="default" eval="False"/>
      <field name="format">Letter</field>
      <field name="orientation">Portrait</field>
      <field name="margin_top">40</field>
      <field name="margin_bottom">20</field>
      <field name="margin_left">5</field>
      <field name="margin_right">5</field>
      <field name="header_line" eval="False"/>
      <field name="header_spacing">40</field> 
      <field name="dpi">90</field>
      <field name="disable_shrinking" eval="True"/>
    </record>

    <record id="paperformat_lims_test_report" model="report.paperformat">
        <field name="name">Informe de Ensayo (LIMS)</field>
        <field name="default" eval="False"/>
        <field name="format">Letter</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">48</field>
        <field name="margin_bottom">18</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">45</field>  
        <field name="dpi">90</field>
        <field name="disable_shrinking" eval="True"/>
    </record>
    
    <record id="paperformat_dymo_label" model="report.paperformat">
        <field name="name">Etiqueta DYMO 1-1/8" x 3-1/2" (Horizontal)</field>
        <field name="default" eval="False"/>
        <field name="format">custom</field>
        <field name="page_height">29</field>
        <field name="page_width">89</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">3</field>
        <field name="margin_right">3</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">203</field>
        <field name="disable_shrinking" eval="True"/>
    </record>

  </data>
</odoo>